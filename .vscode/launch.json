{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "processId": "${command:PickProcess}",
            "port": 5858
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp copy-uncompressed-assets-fonts",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "args": [
                "copy-uncompressed-assets-fonts"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp serve-build-tc",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "args": [
                "serve-build-tc"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp serve-dev-test-template-cache",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "args": [
                "serve-dev-test-template-cache"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp serve-dev",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "args": [
                "serve-dev","--theme","materiallab"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}\\src\\server\\app.js"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Port",
            "address": "localhost",
            "port": 9090
        }
    ]
}